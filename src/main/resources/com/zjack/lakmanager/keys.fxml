<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="260.0" minWidth="450.0" prefHeight="260.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zjack.lakmanager.Controller">
   <children>
      <HBox alignment="CENTER_LEFT" minWidth="450.0" prefHeight="33.0" prefWidth="450.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="create" mnemonicParsing="false" onAction="#create" text="Create">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="remove" mnemonicParsing="false" onAction="#remove" text="Remove">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="copy" mnemonicParsing="false" onAction="#copy" text="Copy Key">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ListView fx:id="listview" editable="true" minWidth="450.0" prefHeight="200.0" prefWidth="450.0" style="-fx-background-color: white;" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
